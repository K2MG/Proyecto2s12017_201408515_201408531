<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ottoniel.clientecalendar.inicio">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOGIN"
        android:id="@+id/botonLogin"
        android:layout_alignParentTop="true"
        android:layout_marginTop="134dp"
        android:layout_alignLeft="@+id/botonRegistrarse"
        android:layout_alignStart="@+id/botonRegistrarse"
        android:layout_alignRight="@+id/botonRegistrarse"
        android:layout_alignEnd="@+id/botonRegistrarse"
        android:textSize="20dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REGISTRARSE"
        android:id="@+id/botonRegistrarse"
        android:layout_below="@+id/botonLogin"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        android:textSize="20dp" />
</RelativeLayout>
